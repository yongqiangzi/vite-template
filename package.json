{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite",
    "build:prod": "node script/updateVersion.js && run-p type-check build-prod",
    "build:dev": "node script/updateVersion.js && run-p type-check build-dev",
    "build-prod": "rimraf build && vite build --mode production",
    "build-dev": "rimraf build && vite build --mode test",
    "preview": "vite preview",
    "preview:build": "pnpm build:prod && vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "pnpm run lint:style && pnpm run lint:js",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit"
  },
  "engines": {
    "node": ">=16.20.0"
  },
  "browserslist": [
    "> 1%",
    "not ie <= 8",
    "last 2 version"
  ],
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.0",
    "ant-design-vue": "^4.0.3",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.0",
    "increase-memory-limit": "^1.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.0.1",
    "typescript": "~5.1.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.6",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^1.8.6"
  }
}
